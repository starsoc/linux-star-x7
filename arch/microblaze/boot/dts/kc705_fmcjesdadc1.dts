			clocks = <&clk_bus_0>;
			compatible = "xlnx,xps-uartlite-1.00.a";
			current-speed = <115200>;
			device_type = "serial";
			interrupt-parent = <&axi_intc>;
			//interrupts = <-1 0>;
			port-number = <1>;
			reg = <0x41400000 0x1000>;
